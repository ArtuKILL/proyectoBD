<p id="notice"><%= notice %></p>

<h1>Area Intereses</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @area_intereses.each do |area_interes| %>
      <tr>
        <td><%= link_to 'Show', area_interes %></td>
        <td><%= link_to 'Edit', edit_area_interes_path(area_interes) %></td>
        <td><%= link_to 'Destroy', area_interes, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

