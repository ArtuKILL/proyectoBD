<p id="notice"><%= notice %></p>

<h1>Detalle Servicios</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @detalle_servicios.each do |detalle_servicio| %>
      <tr>
        <td><%= link_to 'Show', detalle_servicio %></td>
        <td><%= link_to 'Edit', edit_detalle_servicio_path(detalle_servicio) %></td>
        <td><%= link_to 'Destroy', detalle_servicio, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

