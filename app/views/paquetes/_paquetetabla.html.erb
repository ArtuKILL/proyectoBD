<% @agencies.each do |agency|%>
<div class="grid-x">
        <div class="cell large-6"><h1 class="text table"><%= agency.nombre_agencia %></h1></div>
        <div class="cell large-3"></div>
        <div class="cell large-3">
        <%= button_to new_agency_paquete_path(agency, @paquete), :method => :get do%>
            <button class="button-hover-like button rounded text socios-button expanded"><span>Agregar Paquete</span><i class="fa fa-heart"></i></button>
       <% end %> 
        </div>
</div>
<br>


<% @paquetes.each do |paquete| %>

<% if agency.id == paquete.id_agencia %>
<hr>
<div class="grid-x ">
    <div class="cell large-1 "></div>
    <div class="cell large-4 title-form  table">
        <%= link_to  agency_paquete_path(agency, paquete), :method => :get do %>
        <p class="text size"><%= paquete.nombre_paquete %></p>
        <% end %>
    </div>
    <div class="cell large-3"></div>
    <div class="cell large-1 edit-icon">
    <%= link_to  agency_paquete_paises_path(agency, paquete), :method => :get do %>
        <img src="data:image/svg+xml;base64," width="25px" />
    <% end %>
    </div>
     <div class="cell large-1 edit-icon">
     
        <img src="data:image/svg+xml;base64," width="20px"/>
 
     </div>
    <div class="cell large-1 edit-icon">
    <%= link_to  edit_agency_paquete_path(agency, paquete) do %>
        <img src="data:image/svg+xml;base64," width="25px" />
     <% end %>
    </div>
    <div class="cell large-1 trash-icon ">
    <%= link_to  agency_paquete_path(agency, paquete), :method => :delete, data: { confirm: 'Are you sure?' } do %>
        <img src="data:image/svg+xml;base64," width="25px" />
     <% end %>
    </div>
</div>
<% end %>
<% end %>
<hr>
<br>

<% if !(agency.id = agency.paquetes.find_by(params[:id_agencia])) %>
    <p class="text title-form size">*No hay paquetes asociados a esta agencia*</p>
<% end %>
<br>
<% end %>


