<% @agency.paquetes.each do |paquete| %>
<div class="grid-x  box-rounded">

    <div class="cell large-7">
        <%= image_tag("tristan-colangelo-d4OnMIl5OYk-unsplash.jpg",size: "344x350", :class => 'rounded-a')%>
    </div>
    <div class="cell large-5 space-paquete space-again">
        <h3 class="title title-form table"><%= paquete.nombre_paquete %></h3>

        <p Class="text socios-button"><%= paquete.descripcion %></p>
        <br>
        <div class="grid-x ">
            <div class="cell large-2 table"><img
                    src="data:image/svg+xml;base64,"
                    width="30px" /></div>
            <div class="cell large-3">
                <p class="text"><%= paquete.duracion_dias %> dias/ noches</p>
            </div>
            <div class="cell large-2"></div>
            <div class="cell large-2 table">
                <img src="data:image/svg+xml;base64,"
                    width="34px" /></div>
            <div class="cell large-3">
                <p class="text">Para: <%= paquete.cant_personas %> personas</p>
            </div>
        </div>
        <div class="grid-x">
            <div class="cell large-2">
                <%= button_to "Comprar Paquete", new_paquete_paquete_contrato_url(paquete.id), method: :get, class: "button rounded" %>
            </div>
        </div>
    </div>
</div>

<br>
<br>
<br>
<% end %>