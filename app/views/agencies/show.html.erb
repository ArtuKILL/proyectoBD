
<br>
<br>
<div class="grid-x">
    <div class="cell large-5">
        <%= image_tag("viajes-el-corte-ingles_img81859t0.jpg", :class => 'rounded-a')%>
    </div>
    <div class="cell large-1"></div>
    <div class="cell large-6">
        <h1 class="title title-form"><%= @agency.nombre_agencia %></h1>
        <br>
        <p class="text size"><i>"<%= @agency.descripcion %>"</i></p>
        <br>
        <div class="grid-x">
            <div class="cell large-1"></div>
            <div class="cell large-4">
               <div class="grid-x">
                    <div class="cell large-4">
                        <img src="data:image/png;base64," width="50px"/>
                    </div>
                    <div class="cell large-8">
                        <p class="text size"><%= @agency.alcance %></p>
                    </div>
                </div> 
            </div>
            <div class="cell large-1"></div>
            <div class="cell large-5">
                <div class="grid-x">
                    <div class="cell large-3">
                        <img src="data:image/svg+xml;base64," width="45px" />
                    </div>
                    <div class="cell large-9">
                        <p class="text size"><%= @agency.tipo %></p>
                    </div>
                </div>
            </div>
            <div class="cell large-1"></div>
        </div>
        <br>
        <div class="grid-x">
            <div class="cell large-1">
                <img src="data:image/svg+xml;base64," width="35px" />
            </div>
            <div class="cell large-9">
                <p class="text size"><i><%= @agency.url %></i></p>
            </div>
        </div>
    </div>
</div>

<br>
<hr>
<div class="grid-x size">
      <div class="cell large-5">
            <p class="text size">Socios de esta Agencia:</p>
            <hr>
             <% @agency.socio.each do |socio| %> 
                <% if socio.fecha_final.nil? %>
                   <div class="grid-x">
                        <div class="cell large-1"></div>
                        <div class="cell large-4">
                            <p class="text size"><%= socio.agencia2.nombre_agencia %></p>
                        </div>
                        <div class="cell large-2"></div>
                        <div class="cell large-4">
                            <%= button_to "Cerrar Sociedad", agencies_cerrar_sociedad_url(socio.fecha_inicial), class: "hollow button alert rounded expanded text socios-button" %>      
                        </div>
                        <div class="cell large-1"></div>
                    </div>
                <% end %>
            <% end %>
 


      </div>
      <div class="cell large-2"></div>
      <div class="cell large-5 text">
      <p class="text size">Agregar Socios:</p>
      <hr>
        <%=render "socios/form" , socio: @agency.socios.build%>
      </div>
</div>
<hr>

                 
<br>
<br>
<%= render  "paquetes/paquetesdeagencia" %>