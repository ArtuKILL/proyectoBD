
<% @agencies.each do |agency| %>
<hr>
<div class="grid-x ">
    <div class="cell large-1 "></div>
    <div class="cell large-4 title-form  table">
        <%= link_to  agency_path(agency), :method => :get do %>
        <p class="text size"><%= agency.nombre_agencia %></p>
        <% end %>
    </div>
    <div class="cell large-3 "></div>
    <div class="cell large-2 edit-icon">
    <%= link_to  edit_agency_path(agency) do %>
        <img src="data:image/svg+xml;base64," width="25px" />
     <% end %>
    </div>
    <div class="cell large-2 trash-icon ">
    <%= link_to  agency_path(agency), :method => :delete, data: { confirm: 'Are you sure?' } do %>
        <img src="data:image/svg+xml;base64," width="25px" />
     <% end %>
    </div>

</div>
<% end %>
<hr>