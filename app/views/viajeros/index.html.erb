<h1>Viajeros</h1>

<table>
  <thead>
    <tr>
      <th>Primer nombre</th>
      <th>Segundo nombre</th>
      <th>Primer apellido</th>
      <th>Segundo apellido</th>
      <th>Genero</th>
      <th>Fecha nacimiento</th>
      <th>Correo viajero</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @viajeros.each do |viajero| %>
      <tr>
        <td><%= viajero.primer_nombre %></td>
        <td><%= viajero.segundo_nombre %></td>
        <td><%= viajero.primer_apellido %></td>
        <td><%= viajero.segundo_apellido %></td>
        <td><%= viajero.genero %></td>
        <td><%= viajero.fecha_nacimiento %></td>
        <td><%= viajero.correo_viajero %></td>
        <td><%= link_to 'Show', viajero %></td>
        <td><%= link_to 'Edit', edit_viajero_path(viajero) %></td>
        <td><%= link_to 'Destroy', viajero, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Viajero', new_viajero_path %>
