<% @viajeros.each do |viajero| %>
<hr>
<div class="grid-x ">
    <div class="cell large-1 "></div>
    <div class="cell large-4 title-form  table">
        <p class="text size"><%= viajero.primer_nombre %></p>
    </div>
    <div class="cell large-3 "></div>
    <div class="cell large-2 edit-icon">
    <%= link_to  edit_agency_path(viajero.id), :method => :edit do %>
        <img src="data:image/svg+xml;base64," width="25px" />
     <% end %>
    </div>
    <div class="cell large-2 trash-icon ">
    <%= link_to  agency_path(viajero), :method => :delete, data: { confirm: 'Are you sure?' } do %>
        <img src="data:image/svg+xml;base64," width="25px" />
     <% end %>
    </div>

</div>
<% end %>
<hr>