
<%= form_with(model:[@agency, socio], local:true) do |form| %>

      <%= render "clientes/shared/error_messages", resource: socio%>

      <%= form.label :fecha_inicial, "Fecha inicial" %>
      <%= form.date_field :fecha_inicial, class:"form-control input-sm" %>
      <%= form.collection_select :agencia2_id, Agency.order(:nombre_agencia), :id, :nombre_agencia,   {}  ,{:class => "size-letter"} %>
      <div class="grid-x">
            <div class="cell large-2"></div>
            <div class="cell large-8"><%= form.submit class: "button text size expanded rounded"%></div>
            <div class="cell large-2"></div>
      </div>
<% end %>
  